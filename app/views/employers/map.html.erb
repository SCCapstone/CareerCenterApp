<h1>Map - <%= current_con.name %> </h1>
<h2><a href= "/employers/directions"><font size= "9">Directions to <%= current_con.name%></font></a></h2>
<br>
<div id = "map_container">
<%= image_tag("/uploads/maps/#{current_con.map}", class: "maps") %>
</div>

<% if current_user && admin? %>
  <%= form_tag("/employers/map_upload", multipart: true) do %>
  <%= label_tag(:q, "Upload:") %>
  <%= file_field_tag 'map' %>
  <%= submit_tag("Upload") %>
  <% end %>
<% end %>
